<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Managers</title>
  <link rel="stylesheet" href="/css/adminmodulo/manager.css">
  <link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css" />
  <link rel="stylesheet" href="/css/loginmodule/popup.css">

</head>

<body onload="getData()">

  <%- include('../partials/adminmenubar') %>
  <div class="rightbar">  
    <div class="profilename position-relative">
      <div class="float-start">
        <h3 class="text-center m-3">Managers</h3>
      </div>
      <div class="d-flex position-absolute end-0">
        <div class="bell">
          <i class="fa-solid fa-bell"></i>
        </div>
        
        <div class="profile d-flex" onclick="profileShow()" style="cursor: pointer;">
          <i class="fa-solid fa-user"></i>
          <p><B>Name</B></p>
        </div>
      </div>
    </div>
    <div class="row1 my-4">
      <div class="search float-start mx-3 ">
        <input type="text" placeholder="Search Employee.." class="rounded py-2 px-3 " tabindex="1">
      </div>
      <div class="addemployee">
        <input type="button" value="+  Add Manager" class="btn btn-secondary float-end mx-2 my-2 px-3" tabindex="2" onclick="openPopup()">
      </div>
    </div>
    <div class="my-2">
      <table class="table mx-2 text-center" id="man-table">
        
      </table>
    </div>
    <div class="addtaskcontainer" id="show-detail">
      <form action="" id="manager-form"></form>
    </div>
  </div>
  <div class="addregistrationcontainer" id="open-registration">
    <%- include("../loginmodule/registration")%>
  </div>
    <script src="/js/adminmodulo/manager.js"></script>
    <script src="/js/loginmodule/popup.js"></script>  

</body>
</html>