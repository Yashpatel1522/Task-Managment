<html>

<head>
  <link rel="stylesheet" href="/css/adminmodulo/manager.css">
  <link rel="stylesheet" href="/css/bootstrap.min.css">

  <link rel="stylesheet" href="/css/loginmodule/popup.css">
</head>

<body>

</head>

<body onload="getEmpData()">


  <%- include('../partials/adminmenubar') %>
    <div class="rightbar">
      <%- include('../partials/navigation') %>

      <div class="my-5">
        <div class="row1 my-4">
          <div class="search float-start mx-3 ">
            <input type="text" placeholder="Search Employee.." class="rounded py-2 px-3 " tabindex="1" onchange="searchEmpData(this.value)">
          </div>
          <div class="addemployee">
            <input type="button" value="+  Add Employee" class="btn btn-secondary float-end mx-2 my-2 px-3" tabindex="2"
              onclick="openPopup()">
          </div>
        </div>
      </div>
      <div class="my-2">
        <table class="table mx-2 text-center" id="emp-table">
          
        </table>
      </div>
      <div id="pagination">
        
      </div>
      <div class="addtaskcontainer" id="show-detail">
        <form action="" id="employee-form"></form>
      </div>
    </div>
    <div class="addregistrationcontainer" id="open-registration">
      <%- include("../loginmodule/registration")%>
    </div>
  <script src="/js/loginmodule/popup.js"></script>  

</body>
</html>