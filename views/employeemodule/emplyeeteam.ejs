<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link rel="stylesheet" href="/css/employee/employee.css" />
  <link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css" />
  <link rel="stylesheet" href="/bootstrap_icon_css/bootstrap-icons.min.css">
  <script src="/js/employee/employeeteam.js"></script>
  <script src="/js/employee/employeeprofile.js"></script>
  <script src="/js/bootstrap.js"></script>
  <link rel="stylesheet" href="/sweetalert2/sweetalert2.min.css">
  <script src="/sweetalert2/sweetalert2.min.js"></script>
  <script src="/js/employee/employeenavigation.js"></script>
  <script src="/boxicon/dist/boxicons.js"></script>
  <link rel="stylesheet" href="/boxicon/css/boxicons.min.css">
  <link rel="stylesheet" href="/css/adminmodule/menubar.css">
  <title>Document</title>
</head>

<body>
  <div class="container-div">
    <div class="admin">
      <%- include('../partials/employeemenubar') %>
  
        <div class="rightbar">
          <%- include('../partials/employeenavigation') %>
            <div class="bodypage">
              <div class="my-5">
                <div class="row1 my-4">
                  <div class="search mx-3">
                    <input type="text" placeholder="Search task.." class="rounded py-2 px-3" tabindex="1"
                      oninput="seachresultnew()" />
                  </div>
                </div>
              </div>
              <div class="row my-5 mx-5">
                <div class="cards">
                  <table class="table" style="text-align: center" id="details">
                    <tr>
                      <th>Team Name</th>
                      <th>Members Details</th>
                    </tr>
                  </table>
                </div>
              </div>
            </div>

            <div class="popup-team" id="team">
              <div class="body">
                <div class="modal-dialog" role="document">
                  <div class="modal-content">
                    <div class="modal-header">
                    </div>
                    <div class="modal-body" id="teamdetails"></div>
                  </div>
                </div>
              </div>
            </div>
        </div>
    </div>
  </div>
</body>

</html>


